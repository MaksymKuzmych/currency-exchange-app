{
  "expo": {
    "name": "currency-exchange-app",
    "slug": "currency-exchange-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.currencyexchangeapp"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.currencyexchangeapp"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6721e276-a591-4f17-bf43-a1b12aaa15fe"
      }
    }
  }
}
